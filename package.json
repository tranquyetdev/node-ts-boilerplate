{
  "name": "node-ts-boilerplate",
  "version": "0.0.0",
  "description": "A NodeJS TypeScript project boilerplate with settings and common toolchain in place.",
  "main": "index.ts",
  "author": "Quyet Tran <tranquyet.dev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test:lint": "eslint src --ext .ts --ext .mts",
    "test:format": "prettier --config .prettierrc --check .",
    "test:typescript": "tsc",
    "test:unit": "echo TODO",
    "test:unit:ci": "echo TODO",
    "test:circular-deps": "echo TODO",
    "test:security": "echo TODO",
    "format": "prettier --config .prettierrc --write .",
    "dev": "nodemon src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "volta": {
    "node": "16.19.0",
    "npm": "9.2.0",
    "yarn": "1.22.19"
  }
}
